<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="tc_textview">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingLeft">20dp</item>
        <item name="android:paddingRight">20dp</item>
        <item name="android:layout_marginTop">20dp</item>
        <item name="android:textSize">20dp</item>
        <item name="android:textColor">@color/black</item>
    </style>
    <style name="tenant_account_button">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">-4dp</item>
        <item name="android:layout_marginBottom">-1.1dp</item>
        <item name="android:layout_marginRight">-4dp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:background">@drawable/textview_borderbottom</item>
        <item name="android:drawableLeft">@drawable/ic_logout</item>
        <item name="android:gravity"></item>
        <item name="android:textStyle">bold</item>
        <item name="android:paddingLeft">40dp</item>
    </style>
</resources>